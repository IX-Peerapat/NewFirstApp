<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/titleFragment">

    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.example.newfirstapp.AboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/titleFragment"
        android:name="com.example.newfirstapp.TitleFragment"
        android:label="TitleFragment" />
    <fragment
        android:id="@+id/addContact"
        android:name="com.example.newfirstapp.AddContact"
        android:label="fragment_add_contact"
        tools:layout="@layout/fragment_add_contact" >
        <action
            android:id="@+id/action_addContact_to_contactFragment2"
            app:destination="@id/contactFragment2" />
    </fragment>
    <fragment
        android:id="@+id/contactFragment2"
        android:name="com.example.newfirstapp.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" >
        <action
            android:id="@+id/action_contactFragment2_to_addContact"
            app:destination="@id/addContact" />
    </fragment>
</navigation>